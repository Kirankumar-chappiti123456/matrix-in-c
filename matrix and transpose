#include<stdio.h>

int main()
{
    int a[5][5],r=2,c=3;
    printf("enter the elements in matrix:");
    for(int i=0;i<r;i++)
    {
        for(int j=0;j<c;j++)
        {
            scanf("%d",&a[i][j]);
        }
        
    }
    printf("2 * 3 matrix:\n");
    for(int i=0;i<r;i++)
    {
        for(int j=0;j<c;j++)
        {
            printf(" %d ",a[i][j]);
        }
        printf("\n");
    }
    printf("transpose of a matrix :\n");
    for(int i=0;i<c;i++)
    {
        for(int j=0;j<r;j++)
        {
            printf(" %d ",a[j][i]);
        }
        printf("\n");
    }
}


user values:



#include<stdio.h>

int main()
{
    int a[5][5],r,c;
    printf("enter the no of rows:");
    scanf("%d",&r);
    printf("enter the no of colums:");
    scanf("%d",&c);
    printf("enter the elements in matrix:");
    for(int i=0;i<r;i++)
    {
        for(int j=0;j<c;j++)
        {
            scanf("%d",&a[i][j]);
        }
        
    }
    printf("2 * 3 matrix:\n");
    for(int i=0;i<r;i++)
    {
        for(int j=0;j<c;j++)
        {
            printf(" %d ",a[i][j]);
        }
        printf("\n");
    }
    printf("transpose of a matrix :\n");
    for(int i=0;i<c;i++)
    {
        for(int j=0;j<r;j++)
        {
            printf(" %d ",a[j][i]);
        }
        printf("\n");
    }
}


output:
enter the elements in matrix:1 2 3 4 5 6 
2 * 3 matrix:
 1  2  3 
 4  5  6 
transpose of a matrix :
 1  4 
 2  5 
 3  6 
